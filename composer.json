{
    "name": "meops/laravel-populate",
    "version": "0.1.0",
    "description": "Populate the DB in your Laravel application",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Meops\\Populate\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Meops\\Populate\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "meops"
        }
    ],
    "require": {
        "php": "^8.2|^8.3",
        "laravel/framework": "^11.0"

    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "orchestra/testbench": "^9.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Meops\\Populate\\PopulateServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "archive": {
        "exclude": ["/.github"]
    }
}
